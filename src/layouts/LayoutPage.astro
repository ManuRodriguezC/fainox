---
import Footer from "../components/footer.astro";
import Navbar from "../components/navbar.astro";
import Layout from "../layouts/Layout.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <Navbar />

  <div class="w-full h-auto mt-20 xl:mt-20">
    <slot />
  </div>

  <Footer />
</Layout>
